class Sched
  def report_event(env)
    body = env.request.body.not_nil!.gets_to_end
    env.set "log", body
  rescue e
    env.response.status_code = 500
    @log.warn({
      "message" => e.to_s,
      "error_message" => e.inspect_with_backtrace.to_s
    }.to_json)
  end
end
